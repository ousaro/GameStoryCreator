{
  "expo": {
    "name": "GameStoryCrafter",
    "slug": "GameStoryCrafter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LogoIcon.png",
    "scheme": "GameStoryCrafter",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/SplashScreenTransparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#161622"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.ousaro.GameStoryCrafter"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.ico"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b151bc02-87b4-4e90-95e6-e3ab9c1ea82b"
      }
    }
  }
}
